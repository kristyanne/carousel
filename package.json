{
  "name": "carousel",
  "version": "1.0.0",
  "description": "Vanilla JS Carousel",
  "main": "index.js",
  "scripts": {
    "bundle": "browserify src/js/index.js -t babelify --outfile dist/carousel.js",
    "sass": "sass src/scss/carousel.scss dist/carousel.css",
    "dev": "sass --watch src/scss:dist",
    "build": "npm run bundle && npm run sass"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kristyanne/carousel.git"
  },
  "author": "Kristy Nolan <https://github.com/kristyanne>",
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/kristyanne/carousel/issues"
  },
  "homepage": "https://github.com/kristyanne/carousel#readme",
  "engines": {
    "node": ">=4"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.9.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0"
  }
}
